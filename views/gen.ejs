<%- include ('template/header.ejs')-%>


<div style="height: 100vh;width: 100vw;">
    <div class="row">
        <div class="col-lg-5">
            <h1>GeneticX</h1>
            <p> <%= gen %> </p>
        </div>
        <div class="col-lg-5"><img src="/images/clip-dancer.png" height="50%"></div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <h1>Co-ordinators</h1>
        <ul>
            <% for(var i=0;i<arr.length; i++){ %>
            <% if(arr[i].role=="C") {%>
            <li>
                <%= arr[i].name %>
            </li>
            <% } %>
            <% } %>


        </ul>

        <h1>Members</h1>
        <ul>
            <% for(var i=0;i<arr.length; i++){ %>
            <% if(arr[i].role=="M") {%>
            <li>
                <%= arr[i].name %>
            </li>
            <% } %>
            <% } %>


        </ul>
        <h1>Upcoming Events</h1>
        <ul>
            <% for(var i=0;i<arr.length; i++){ %>
            <% if(arr[i].role=="E") {%>
            <li>
                <%= arr[i].name %>
            </li>
            <% } %>
            <% } %>


        </ul>
    </div>
</div>
<%- include ('template/footer.ejs') %>